<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
	<display-name>springTest</display-name>

	<servlet>
		<servlet-name>springapp</servlet-name>
		<servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<servlet-mapping>
		<servlet-name>springapp</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>dispatcher</servlet-name>
		<servlet-class>au.org.aurin.dispatcher.Dispatcher</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>dispatcher</servlet-name>
		<url-pattern>/dispatcher</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>GeoserverProxy</servlet-name>
		<servlet-class>au.org.housing.proxy.servlet.ProxyServletWithAuth</servlet-class>
		<init-param>
			<param-name>ProxyTo</param-name>
			<param-value>http://localhost:8086/geoserver</param-value>
		</init-param>
		<!--   <init-param>
			<param-name>HostHeader</param-name>
			<param-value>Basic YXVyaW46YXVyaW5hY2Nlc3M=</param-value>
		</init-param>  -->
		<init-param>
			<param-name>Prefix</param-name>
			<param-value>/geoserver</param-value>
		</init-param>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>GeoserverProxy</servlet-name>
		<url-pattern>/geoserver/*</url-pattern>
	</servlet-mapping>
	
	
	
	
